{
    "name": "mobi-market/checkmend",
    "description": "Checkmend API laravel package",
    "require": {
        "php": "^7.4|^8.0",
        "guzzlehttp/guzzle": "^6.3|^7.3",
        "illuminate/support": ">=5.8 <10.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0"
    },
    "license": "proprietary",
    "authors": [
        {
            "name": "Scott-David Jones",
            "email": "scott.jones@autumndev.co.uk"
        }
    ],
    "autoload": {
        "psr-4": {
            "Autumndev\\Checkmend\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Autumndev\\Checkmend\\CheckmendServiceProvider"
            ],
            "aliases": {
                "Checkmend": "Autumndev\\Checkmend\\CheckmendFacade"
            }
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
